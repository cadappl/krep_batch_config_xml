      <pattern name="CyanogenMod/ion" replace="platform/external/ion" />
      <pattern name="CyanogenMod/AndroidAsync" replace="platform/packages/apps/AndroidSync" />
      <pattern name="^CyanogenMod/.*$" value="~_~/~,~CyanogenMod/android/device/~device/~,~CyanogenMod/android/~platform/~,~CyanogenMod/cm/~platform/vendor/" />
