<?xml version="1.0" encoding="utf-8"?>
<projects>
  <global_option name="keep_name" />
  <global_option name="all" />

  <!-- For CM project, the git name need convert to the aosp one for the simple maintenance -->
  <project name="cm" group="android">
    <option name="manifest" value="https://github.com/CyanogenMod/android.git" />
    <option name="manifest-branch" value="cm-13.0" />
    <option name="schema" value="repo-mirror" />
    <option name="relative-dir" value="cm-13.0" />
    <option name="refs" value="cm" />
    <option name="repo_create" value="false" />
    <!-- don't add "refs" for git-repo and tools/repo -->
    <exclude-patterns category="project">
      <pattern value="git-repo" />
      <pattern value="tools/repo" />
    </exclude-patterns>
    <replace-patterns category="project-rp">
      <pattern name="^CyanogenMod/android$" replace="platform/manifest" />
      <pattern name="CyanogenMod/android_external_ahbottomnavigation" replace="platform/vendor/cm/external/ahbottomnavigation" />
      <pattern name="CyanogenMod/android_external_android-visualizer" replace="platform/packages/apps/AndroidVisualizer" />
      <pattern name="CyanogenMod/android_external_ant-wireless_ant_native" replace="platform/external/ant-wireless/ant_native" />
      <pattern name="CyanogenMod/android_external_ant-wireless_ant_service" replace="platform/external/ant-wireless/ant_service" />
      <pattern name="CyanogenMod/android_external_bash" replace="platform/external/bash" />
      <pattern name="CyanogenMod/android_external_brctl" replace="platform/kernel/bridge-utils" />
      <pattern name="CyanogenMod/android_external_cmsdk-api-coverage" replace="platform/vendor/cm/external/sdk-api-coverage" />
      <pattern name="CyanogenMod/android_external_cyanogen_UICommon" replace="platform/packages/apps/UICommon" />
